<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:sh.Creator.Themes.Cad2012">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ScrollBar.xaml"/>
        <ResourceDictionary Source="TreeView.xaml"/>
        <ResourceDictionary Source="Expander.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="{x:Type TreeView}" BasedOn="{StaticResource tv}"/>
    <Style TargetType="{x:Type ScrollBar}" BasedOn="{StaticResource DefaultScrollBar}"/>
    <Style TargetType="{x:Type Expander}" BasedOn="{StaticResource expander}"/>
</ResourceDictionary>